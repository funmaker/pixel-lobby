{
	"name": "Boilerplate",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.1.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"repository": "https://github.com/funmaker/webapp-boilerplate",
	"scripts": {
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r static dist/",
		"build:prod-package": "babel-node webpack/package.babel.js"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"body-parser": "^1.19.0",
		"bson": "^4.0.2",
		"canvas": "^2.4.1",
		"chalk": "latest",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.4",
		"detect-node": "^2.0.4",
		"express": "^4.16.4",
		"express-handlebars": "^3.0.2",
		"express-promise-router": "^3.0.3",
		"express-ws": "^4.0.0",
		"fs-extra": "^7.0.1",
		"morgan": "^1.9.1",
		"prop-types": "^15.7.2",
		"query-string": "^6.5.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-hot-loader": "^4.8.4",
		"react-router": "^5.0.0",
		"react-router-dom": "^5.0.0",
		"source-map-support": "^0.5.12",
		"uuid": "latest",
		"youtube-player": "^5.5.2",
      "lodash": "latest"
    },
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/polyfill": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.4.4",
		"@hot-loader/react-dom": "^16.8.6",
		"babel-loader": "^8.0.5",
		"css-loader": "^2.1.1",
		"handlebars": "^4.1.2",
		"handlebars-loader": "^1.7.1",
		"mini-css-extract-plugin": "^0.6.0",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^7.1.0",
		"start-server-webpack-plugin": "^2.2.5",
		"style-loader": "^0.23.1",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.1",
		"webpack-dev-middleware": "^3.6.2",
		"webpack-hot-middleware": "^2.24.4",
		"webpack-node-externals": "^1.7.2"
	},
	"port": 3900
}
